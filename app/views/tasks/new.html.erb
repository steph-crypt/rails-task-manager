<%= form_for(@task) do |f| %>
<h1><%= f.label :title %></h1>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :created_at %>
  <%= f.text_field :created_at %>
  <%= f.label :updated_at %>
  <%= f.text_field :updated_at %>
  <%= f.submit %>
<% end %>

